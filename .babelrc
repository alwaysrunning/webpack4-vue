{
  "presets": [
    [
      "@babel/preset-env", 
      {
        "useBuiltIns": "usage",
        "corejs": 2,
        "targets": {
          "browsers": [
            "> 1%", 
            "last 2 versions", 
            "not ie <= 8"
          ]
        }
    }]
  ],
  "plugins": [
    // "@babel/plugin-proposal-object-rest-spread",
    // "@babel/plugin-syntax-dynamic-import",
    // "@babel/plugin-transform-object-assign",
    "@babel/plugin-transform-runtime"
  ] 
}
